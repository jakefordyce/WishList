<p id="notice"><%= notice %></p>

<h1>Collab Requests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @collab_requests.each do |collab_request| %>
      <tr>
        <td><%= link_to 'Show', collab_request %></td>
        <td><%= link_to 'Edit', edit_collab_request_path(collab_request) %></td>
        <td><%= link_to 'Destroy', collab_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Collab Request', new_collab_request_path %>
