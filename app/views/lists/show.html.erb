<%= link_to 'Edit', edit_list_path(@list) %> |
<%= link_to 'Back', lists_path %>

<% @items.each do |item| %>
  <div>
  <%= item.name  %>
  <%= item.url %>
  </div>
<% end %>

<% if user_signed_in? %>
  <%= render 'comments/form', comment: @comment, list_id: @list.id %>
<% end %>

<%= render partial: 'comments/comment', collection: @comments %>
