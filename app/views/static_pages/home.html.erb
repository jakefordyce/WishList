<% if user_signed_in? %>
  <h3>Your Lists</h3>
  <%= link_to "Create a new list", new_list_path %>
  <br/>
  <br/>
  <% @lists.each do |list| %>
    <div>
      <p><%= link_to list.name, list_path(list) %></p>
    </div>
  <% end %>
  <h3>Your Collabs</h3>
  <% @collabs.each do |list| %>
    <div>
      <p><%= link_to list.name, list_path(list) %></p>
    </div>
  <% end %>
<% end %>
