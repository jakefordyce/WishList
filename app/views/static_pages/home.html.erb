<% if user_signed_in? %>
  <h5>Your Lists</h5>
  <% @lists.each do |list| %>
    <div>
      <p><%= link_to list.name, list_path(list) %></p>
    </div>
  <% end %>
  <h5>Your Collabs</h5>
  <% @collabs.each do |list| %>
    <div>
      <p><%= link_to list.name, list_path(list) %></p>
    </div>
  <% end %>
<% end %>
